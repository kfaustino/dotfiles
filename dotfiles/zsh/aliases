# FINDER
alias o="open . &"

# PROCESSES
alias tu="top -o cpu" # PS CPU
alias tm="top -o vsize" #PS Memory
alias trm="top -o rsize" #PS Resident Size Memory

# GIT
alias g="git status -sb"
alias l="git log --oneline --decorate --graph"

# SERVERS
alias redis='redis-server /usr/local/etc/redis.conf'

# Projects
alias pj="m pragmatic_journal"

# Ruby
alias r="bin/rails"
alias be="bundle exec"

# Utilities
alias reload='source ~/bin/dotfiles/zsh/aliases'
alias ea='mate -w ~/bin/dotfiles/zsh/aliases && reload'
